{
  "name": "@eventstore/sdk-ts",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepare": "pnpm run gen && pnpm run build",
    "gen": "(cd ../.. && make gen-ts)",
    "example": "tsx src/examples/typed-basic.ts",
    "typed-example": "tsx src/examples/typed-basic.ts",
    "runtime-example": "tsx src/examples/basic.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.1",
    "@grpc/proto-loader": "^0.7.13",
    "long": "^5.2.3",
    "protobufjs": "^7.5.0"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "@types/node": "^22.5.2",
    "ts-proto": "^1.172.0",
    "tsx": "^4.16.2"
  }
}
